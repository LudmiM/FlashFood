{
  "name": "flash-food",
  "version": "1.0.0",
  "description": "Basics files and folders to start to develop a server with Express and Sequelize, MVC type",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build",
    "server": "nodemon src/index.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:create": "sequelize db:drop & sequelize db:create & sequelize db:migrate & sequelize db:seed:all",
    "db:reset": "sequelize db:migrate:undo:all & sequelize db:migrate & sequelize db:seed:all"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.3",
    "axios": "1.6.8",
    "bcryptjs": "2.4.3",
    "bootstrap": "5.3.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-session": "1.17.3",
    "express-validator": "7.0.1",
    "http-errors": "2.0.0",
    "method-override": "3.0.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.9.1",
    "nodemailer": "6.9.13",
    "nodemon": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6.22.3",
    "sequelize": "6.37.0"
  },
  "devDependencies": {
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "8.55.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "vite": "5.0.8"
  }
}
